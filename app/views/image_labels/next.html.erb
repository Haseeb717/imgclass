<div id="imgframe">
<img class="img-responsive" id="mainimage" src="<%= @unlabeled.image.url %>">
</div>
<div class="progress" style="width:500px;height:20px;">
<div class="progress-bar" role="progressbar" aria-valuenow="<%= 100- (@unlabeled.image.image_set.image_label_set.percent_remaining/2) %>"
aria-valuemin="0" aria-valuemax="100" style="width:<%= 100- (@unlabeled.image.image_set.image_label_set.percent_remaining/2) %>%">
  <span class="sr-only">70% Complete</span>
</div>
</div>
<div id="btncontainer" class="container">
  <%= form_for @unlabeled do |f| %>
  <div class="row">
    <% @unlabeled.image.image_set.image_label_set.label_set.labels.each do |label| %>
    <div class="col-sm-4">
        <button value="<%= label.id %>" name="label_id" type="submit" class="btn btn-default btn-lg btn-block"><%= label.text %></button>
      </div>
    <% end %>
  </div>
  <% end %>
</div>
